<h1>
  Time Entries
</h1>

  <%= form_tag "/time_entries", method:"post" do  %>
<table>
  <tr>
    <td> 
      date
    </td>
    <td>
      <input type = "text" name="date"/>
  </td>
</tr>

  <tr>
    <td>
      time_in
    </td> 
    <td>
      <input type = "text" name="time_in"/>
  </td>
</tr>

  <tr>
    <td>
      time_out
    </td>
    <td>
      <input type = "text" name="time_out"/>
  </td>
</tr>

  <tr>
    <td>
      accomplishments 
      </td>
      <td>
        <input type = "text" name="accomplishments"/>
  </td>

</tr>
  <tr>
    <td>
      remarks
    </td>
    <td>
      <input type = "text" name="remarks"/>
  </td>
</tr>
</table>
<input type = "submit" value="submit"/>
<%end%>
