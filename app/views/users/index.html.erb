<h1>User List</h1>
<% if flash[:notice]%>
  <%=flash[:notice]%>
<%end%>
<br
<table>
  <tr>
    <th>
      First Name:
    </th>
    <th>
    Middle Name:
    </th>
    <th>
      Last Name:
    </th>
    <th>
      Course:
    </th>
    <th>
      Year:
    </th>
    <th>
      Company:
    </th>
    <th>
      Started At:
    </th>
    <th>
      Ended At:
    </th>

  </tr>
  <% @users.each do |users|%>
    <tr>
    <td>
      <%=users.first_name%>
    </td>
    <td>
      <%=users.middle_name%>
    </td>
    <td>
      <%=users.last_name%>
    </td>
    <td>
      <%=users.course%>
    </td>
    <td>
      <%=users.year%>
    </td>
    <td>
      <%=users.company%>
    </td>
    <td>
      <%=users.started_at%>
    </td>
    <td>
      <%=users.ended_at%>
    </td>
    </tr>
  <%end%>
</table>
<a href="/users/new">Register for an account</a>
