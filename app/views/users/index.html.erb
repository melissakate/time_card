<br>
  <div class ="well well-large">
    <h1>User List</h1>
  </div>
<div class="well well-large">
<% if flash[:notice]%>
<div class="alert alert-info">
  <b> Notice: </b><%=flash[:notice]%>
</div>
<%end%>
<table class ="table table-striped">
  <tr>
    <th>
      User ID:
    </th>
    <th>
      First Name:
    </th>
    <th>
      Middle Name:
    </th>
    <th>
      Last Name:
    </th>
    <th>
      Course:
    </th>
    <th>
      Year:
    </th>
    <th>
      Company:
    </th>
    <th>
      Started At:
    </th>
    <th>
      Ended At:
    </th>
    <th>
    </th>

  </tr>
  <% @users.each do |user|%>
    <tr>
      <td>
        <%=user.user_id%>
      </td>
      <td>
        <%=user.first_name%>
      </td>
      <td>
        <%=user.middle_name%>
      </td>
      <td>
        <%=user.last_name%>
      </td>
      <td>
        <%=user.course%>
      </td>
      <td>
        <%=user.year%>
      </td>
      <td>
        <%=user.company%>
      </td>
      <td>
        <%=user.started_at%>
      </td>
      <td>
        <%=user.ended_at%>
      </td>
      <td>
        <%= link_to "Show", "/users/#{user.id}" , method: "get" %>
        <%= link_to "Edit", "/users/#{user.id}/edit" , method:"get"%>
        <%= link_to "Delete", "/users/#{user.id}", method: "delete", confirm: "Are you sure you want to delete this user?" %>
      </td>
    </tr>
  <%end%>
</table>
<%= link_to "Register for an account" , new_user_path ,class:"btn btn-primary" %>
</div>
<div class="div div-inverse"> </div>
